		<function name="getallcontacts" id="Function_28582dce" isSystem="false" >
			<string><![CDATA[CREATE OR REPLACE FUNCTION public.getallcontacts()
 RETURNS refcursor
 LANGUAGE plpgsql
AS $function$
    DECLARE
      contacts refcursor;           -- Declare cursor variables                         
    BEGIN
      OPEN contacts FOR 
      SELECT c.contact_id,
					c.lastname ,
					c.firstname ,
					c.code ,
					c.email, 
					null as "roles", 
					c.created_by,
					c.created_date, 
					c.modified_by, 
					c.modified_date 
			from contact c;
      RETURN contacts;
    END;
$function$
]]></string>
		</function>
